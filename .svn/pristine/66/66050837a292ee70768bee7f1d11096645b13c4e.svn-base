#main .main_left{
    float: left;
    width: 690px;
    min-height: 800px;
}
#main .main_left .person_info{
    height: 220px;
    background: #fafafa;
    border-bottom: 1px solid #ccc;
}
#main .main_left .person_info dl dt{
    float: left;
    padding:10px 0 0 10px;
}
#main .main_left .person_info dl dt img{
    display: block;
}
#main .main_left .person_info dl dd{
    width: 450px;
    float: left;
}
#main .main_left .person_info dl dd.name{
    padding:15px 0 0 20px;
    font-size: 20px;
    font-weight:bold;
    color: #049;
}
#main .main_left .person_info dl dd.intro{
    padding: 10px 0 0 20px;
    font-size: 14px;
    color: #666;
}
#main .main_right{
    float: right;
    width: 269px;
    background: #f7f7f7;
    min-height: 800px;
    border-left:1px solid #ddd;
}
